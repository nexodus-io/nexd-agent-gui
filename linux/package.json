{
  "name": "nexodus-agent-gui",
  "productName": "nexodus-agent-gui",
  "version": "0.1.0",
  "description": "My Electron application description",
  "main": "src/main.ts",
  "scripts": {
    "start": "vite build && electron-forge start",
    "build": "vite build",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make-win": "electron-forge make --platform=win32",
    "publish": "electron-forge publish",
    "package-linux-x64": "electron-packager . dist/nexodus-agent-gui --platform=linux --arch=x64",
    "package-linux-arm64": "electron-packager . dist/nexodus-agent-gui --platform=linux --arch=arm64",
    "package-windows-x64": "electron-packager . nexodus-agent-gui --platform=win32 --arch=x64",
    "package-windows-arm64": "electron-packager . nexodus-agent-gui --platform=win32 --arch=arm64",
    "pack:win": "electron-builder build --win",
    "lint": "eslint --ext .ts,.tsx nexodus-agent-gui"
  },
  "keywords": [],
  "author": {
    "name": "Brent Salisbury",
    "email": "bsalisbu@redhat.com"
  },
  "license": "MIT",
  "build": {
    "appId": "com.nexodus.agent",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.4.2",
    "@electron-forge/maker-deb": "^6.4.2",
    "@electron-forge/maker-rpm": "^6.4.2",
    "@electron-forge/maker-squirrel": "^6.4.2",
    "@electron-forge/maker-zip": "^6.4.2",
    "@electron-forge/plugin-auto-unpack-natives": "^6.4.2",
    "@electron-forge/plugin-vite": "^6.4.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "electron": "26.3.0",
    "electron-packager": "^17.1.2",
    "electron-builder": "^24.6.4",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "ts-node": "^10.0.0",
    "typescript": "~4.5.4",
    "vite-plugin-copy": "^0.1.6"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  }
}
